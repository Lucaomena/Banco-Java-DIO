
public class Main {

	public static void main(String[] args) {
		Cliente lucas = new Cliente();
		lucas.setNome("lucas");
		
		
		Conta cc = new ContaCorrente();
		cc.depositar(100);
		cc.sacar(50);
		
		Conta poupanca = new ContaPoupanca();
		cc.transferir(50, poupanca);
		
		cc.imprimirExtrato();
		poupanca.imprimirExtrato();

	}

}
